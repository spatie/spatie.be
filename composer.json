{
    "name" : "spatie/spatie.be",
    "description" : "Our company website.",
    "keywords" : [
        "spatie",
        "company",
        "website",
        "framework",
        "laravel"
    ],
    "license" : "MIT",
    "type" : "project",
    "require" : {
        "php": ">=7.1.0",
        "abraham/twitteroauth": "^0.9.2",
        "barryvdh/laravel-debugbar": "^3.0",
        "barryvdh/laravel-ide-helper": "^2.4",
        "doctrine/dbal": "^2.8",
        "fideloper/proxy": "~4.0",
        "knplabs/github-api": "^2.8",
        "laravel/framework": "5.6.*",
        "laravel/horizon": "^1.3",
        "laravel/tinker": "~1.0",
        "myclabs/php-enum": "^1.5",
        "pda/pheanstalk": "^3.1",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message": "^1.7",
        "predis/predis": "^1.1",
        "spatie/laravel-backup": "^5.0",
        "spatie/laravel-cors": "^1.2",
        "spatie/laravel-medialibrary": "^7.0",
        "spatie/laravel-menu": "^3.1",
        "spatie/laravel-responsecache": "^4.4",
        "spatie/laravel-tail": "^3.0",
        "spatie/packagist-api": "^1.0",
        "spatie/schema-org": "^2.0",
        "themsaid/laravel-mail-preview": "^2.0",
        "zendframework/zend-feed": "^2.9",
        "zendframework/zend-http": "^2.7"
    },
    "require-dev" : {
        "filp/whoops": "~2.0",
        "friendsofphp/php-cs-fixer": "^2.4",
        "fzaninotto/faker": "~1.4",
        "laravel/envoy": "^1.3",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload" : {
        "classmap" : [
            "database"
        ],
        "psr-4" : {
            "App\\" : "app/"
        },
        "files" : [
            "app/helpers.php"
        ]
    },
    "autoload-dev" : {
        "psr-4" : {
            "Tests\\" : "tests/"
        }
    },
    "scripts" : {
        "deploy" : [
            "envoy run deploy"
        ],
        "deploy-code" : [
            "envoy run deploy-code"
        ],
        "format" : [
            "vendor/bin/php-cs-fixer fix"
        ],
        "post-root-package-install" : [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd" : [
            "php artisan key:generate"
        ],
        "post-autoload-dump" : [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config" : {
        "preferred-install" : "dist",
        "sort-packages" : true,
        "optimize-autoloader" : true
    },
    "extra" : {
        "laravel" : {
            "dont-discover" : []
        }
    }
}
