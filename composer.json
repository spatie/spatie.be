{
    "name": "spatie/spatie.be",
    "description": "Our company website.",
    "keywords": [
        "spatie",
        "company",
        "website",
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "abraham/twitteroauth": "^0.9.2",
        "barryvdh/laravel-debugbar": "^3.0",
        "doctrine/dbal": "^2.8",
        "facade/ignition": "^2.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "knplabs/github-api": "^2.8",
        "laravel/framework": "^7.0",
        "laravel/horizon": "^4.0",
        "laravel/slack-notification-channel": "^2.0",
        "laravel/socialite": "^4.3",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "livewire/livewire": "^1.0",
        "myclabs/php-enum": "^1.5",
        "pda/pheanstalk": "^4.0",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message": "^1.7",
        "predis/predis": "^1.1",
        "spatie/laravel-backup": "^6.8",
        "spatie/laravel-medialibrary": "^7.12",
        "spatie/laravel-menu": "^3.4",
        "spatie/laravel-stubs": "^1.1",
        "spatie/laravel-tail": "^4.2",
        "spatie/packagist-api": "^1.0",
        "spatie/schema-org": "^2.0",
        "spatie/valuestore": "^1.2",
        "zendframework/zend-feed": "^2.9",
        "zendframework/zend-http": "^2.7"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.4",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5",
        "psalm/plugin-laravel": "^1.2",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "deploy": [
            "envoy run deploy"
        ],
        "deploy-code": [
            "envoy run deploy-code"
        ],
        "format": [
            "vendor/bin/php-cs-fixer fix"
        ],
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "@php artisan horizon:publish"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.4.3"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    }
}
